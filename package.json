{
  "name": "jack-of-all-decorators",
  "version": "1.0.0-beta.1",
  "description": "Decorator library for TypeScript. Serialize/deserialzie classes to JSON. Decorate methods with lodash, etc.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "compile-lib": "rm -rf lib/* && tsc",
    "build-test": "rm -rf test/**/*.js && rm -rf test/typings && cd test && typings install && tsc",
    "test": "mocha ./test/index.spec.js -R mocha-lcov-reporter > ./node_modules/coveralls/bin/coveralls.js"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "dependencies": {
    "dateformat": "^1.0.12",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "coveralls": "^2.11.12",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "author": "Patrick Martin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrimart/jack-of-all-decorators-ts"
  },
  "bugs": {
    "url": "https://github.com/patrimart/jack-of-all-decorators-ts/issues"
  },
  "keywords": [
    "ts",
    "typescript",
    "decorators",
    "json",
    "stringify",
    "jsonables",
    "serialize",
    "deserialize",
    "marshal",
    "unmarshal",
    "lodash",
    "dateformat"
  ]
}
